@font-face {
    font-family: 'museo-sans';
    src: url('{{'MuseoSans_300-webfont.eot' | asset_url}}');
    src: url('{{'MuseoSans_300-webfont.eot?#iefix' | asset_url}}') format('embedded-opentype'),
         url('{{'MuseoSans_300-webfont.woff2' | asset_url}}') format('woff2'),
         url('{{'MuseoSans_300-webfont.woff' | asset_url}}') format('woff'),
         url('{{'MuseoSans_300-webfont.ttf' | asset_url}}') format('truetype'),
         url('{{'MuseoSans_300-webfont.svg#museo_sans300' | asset_url}}') format('svg');
    font-weight: normal;
    font-style: normal;
}

