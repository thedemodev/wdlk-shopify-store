{
  "name": "wdlk-ocean-store",
  "version": "3.0.0",
  "description": "WDLK Ocean Shopify Store",
  "scripts": {
    "commit": "commit",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "delete-meta": "babel-node --presets es2015,stage-0 configuration/shopify-api/utils/delete-metafields.js",
    "instagram-auth": "babel-node --presets es2015,stage-0 configuration/instagram/instagram-auth.js",
    "lint": "tslint --project .",
    "pretty": "onchange './src/**/*.ts' -- prettier --write {{changed}}",
    "product-meta": "babel-node --presets es2015,stage-0 configuration/shopify-api/product/send-metafields.js",
    "product-meta-global": "babel-node --presets es2015,stage-0 configuration/shopify-api/product/send-global-metafields.js",
    "product-data": "babel-node --presets es2015,stage-0 configuration/shopify-api/product/prepare-metafields.js",
    "timber": "babel-node --presets es2015,stage-0 configuration/timber.js",
    "slate:build": "npx slate build",
    "slate:deploy": "npx slate deploy -e development",
    "slate:watch": "npx slate watch -e development",
    "dev": "concurrently -k -p \"[{name}]\" -n \"WEBPACK,SHOPIFY,PRETTIER\" \"npx yarn slate:watch\" \"npx yarn webpack\" \"npx yarn pretty\"",
    "webpack": "NODE_ENV=development webpack --config ./webpack.config.js",
    "webpack:production": "NODE_ENV=production npx yarn webpack -p",
    "watch": "babel-node --presets es2015,stage-0 configuration/watch.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/PalmaSwell/wdlk-ocean.git"
  },
  "author": "Woodlike Ocean <info@woodlikeocean.com> (http://woodlikeocean.com/)",
  "contributors": [
    {
      "name": "Mauricio Palma",
      "email": "mauricio.palma@woodlikeocean.com"
    }
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/PalmaSwell/wdlk-ocean/issues"
  },
  "homepage": "https://gitlab.com/PalmaSwell/wdlk-ocean#README",
  "devDependencies": {
    "@commitlint/config-conventional": "^6.1.0",
    "@commitlint/prompt-cli": "^6.1.0",
    "@commitlint/travis-cli": "^6.1.0",
    "@shopify/slate": "^0.13.0",
    "@shopify/slate-tools": "0.13.0",
    "@types/facebook-pixel": "0.0.19",
    "async": "^1.5.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-stage-0": "^6.1.2",
    "babel-preset-stage-1": "^6.22.0",
    "chalk": "^2.3.0",
    "colors": "^1.1.2",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "fork-ts-checker-webpack-plugin": "^0.3.0",
    "happypack": "^4.0.0",
    "husky": "^0.14.3",
    "node-sass": "^4.7.2",
    "npx": "^9.7.1",
    "onchange": "^3.3.0",
    "postcss": "^5.2.17",
    "postcss-loader": "^1.3.3",
    "prettier": "1.10.2",
    "request": "^2.79.0",
    "sass-loader": "^6.0.6",
    "shopify-node-api": "^1.6.1",
    "ts-loader": "^3.2.0",
    "tslint": "^5.9.1",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.7.1",
    "watch": "^0.16.0",
    "webpack": "^2.4.1"
  },
  "dependencies": {
    "@babel/core": "^7.0.0-beta.36",
    "@types/jsonp": "^0.2.0",
    "autoprefixer": "^7.2.3",
    "jsonp": "^0.2.1",
    "yarn": "^1.3.2"
  }
}
