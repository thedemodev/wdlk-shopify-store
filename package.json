{
  "name": "wdlk-ocean-store",
  "version": "1.0.0",
  "description": "WDLK Ocean Shopify Store",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build-sass": "node-sass src/scss/index.scss > assets/app.css",
    "build-js": "browserify src/scripts/index.js -o assets/app.js",
    "build-min-js": "browserify src/scripts/index.js | uglifyjs -o assets/app.min.js",
    "build-min-sass": "node-sass src/scss/index.scss | cleancss -o assets/app.min.css",
    "watch-js": "nodemon -e js -w src/scripts -x 'npm run build-js'",
    "watch-sass": "nodemon -e scss -w src/scss -x 'npm run build-sass'",
    "build": "npm run build-js & npm run build-sass",
    "watch": "npm run watch-js & npm run watch-sass"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/PalmaSwell/wdlk-ocean.git"
  },
  "author": "Woodlike Ocean <info@woodlikeocean.com> (http://woodlikeocean.com/)",
  "contributors": [
    {
      "name": "Mauricio Palma",
      "email": "mauricio.palma@woodlikeocean.com"
    },
    {
      "name": "Maximilian Heiz",
      "email": "heinzmaximilian@googlemail.com"
    }
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/PalmaSwell/wdlk-ocean/issues"
  },
  "homepage": "https://gitlab.com/PalmaSwell/wdlk-ocean#README",
  "devDependencies": {
    "browserify": "^11.0.1",
    "clean-css": "^3.3.9",
    "node-sass": "latest",
    "nodemon": "^1.4.1",
    "uglifyjs": "^2.4.10"
  }
}
