{
  "name": "wdlk-ocean-store",
  "version": "1.5.0",
  "description": "WDLK Ocean Shopify Store",
  "main": "./tasks/watch.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "bundle": "babel-node tasks/bundle.js",
    "commit": "git-cz",
    "css": "babel-node tasks/css.js",
    "delete-meta": "babel-node tasks/shopify-api/utils/delete-metafields.js",
    "flow": "flow",
    "instagram-auth": "babel-node tasks/instagram/instagram-auth.js",
    "product-meta": "babel-node tasks/shopify-api/product/send-metafields.js",
    "product-meta-global": "babel-node tasks/shopify-api/product/send-global-metafields.js",
    "product-data": "babel-node tasks/shopify-api/product/prepare-metafields.js",
    "scripts": "babel-node tasks/scripts.js",
    "timber": "babel-node tasks/timber.js",
    "theme-watch": "theme watch",
    "watch": "babel-node tasks/watch.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/PalmaSwell/wdlk-ocean.git"
  },
  "author": "Woodlike Ocean <info@woodlikeocean.com> (http://woodlikeocean.com/)",
  "contributors": [
    {
      "name": "Mauricio Palma",
      "email": "mauricio.palma@woodlikeocean.com"
    }
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/PalmaSwell/wdlk-ocean/issues"
  },
  "homepage": "https://gitlab.com/PalmaSwell/wdlk-ocean#README",
  "devDependencies": {
    "async": "^1.5.0",
    "babel": "^5.8.29",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.1.2",
    "babel-preset-stage-1": "^6.22.0",
    "babel-runtime": "^5.8.20",
    "babelify": "^7.3.0",
    "bourbon": "^4.3.3",
    "browserify": "^14.1.0",
    "chokidar": "^1.0.5",
    "colors": "^1.1.2",
    "eslint": "^2.13.1",
    "eslint-plugin-flow-check": "^1.1.1",
    "eslint-plugin-flowtype": "^2.32.1",
    "install": "^0.8.8",
    "node-sass": "^4.5.1",
    "npm": "^4.5.0",
    "postcss": "^5.2.16",
    "postcss-color-function": "^3.0.0",
    "postcss-cssnext": "^2.10.0",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-selectors": "^3.0.0",
    "postcss-reporter": "^1.3.1",
    "postcss-scss": "^0.4.1",
    "request": "^2.79.0",
    "shopify-node-api": "^1.6.1",
    "uglifyify": "^3.0.4",
    "watch": "^0.16.0",
    "xo": "^0.18.1"
  },
  "dependencies": {
    "commitizen": "^2.9.6",
    "cz-conventional-changelog-lint": "^0.1.3",
    "fastclick": "^1.0.6",
    "handlebars": "^4.0.5",
    "jquery": "^3.2.1",
    "jsonp": "^0.2.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-lint"
    }
  },
  "xo": {
    "env": [
      "node",
      "browser"
    ],
    "extends": [
      "xo/browser",
      "plugin:flowtype/recommended"
    ],
    "parser": "babel-eslint",
    "plugins": [
      "flow-check",
      "flowtype"
    ],
    "rules": {
      "react/forbid-component-props": [
        "warn"
      ],
      "flow-check/check": "error",
      "flowtype/require-valid-file-annotation": [
        "error",
        "always"
      ]
    },
    "space": true,
    "ignore": [
      "flow-typed/npm/**"
    ]
  }
}
